<Application x:Class="SophiAppCE.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SophiAppCE"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Localization/EN.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/General/Animations.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/General/ControlsSizes.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/General/IconsPath.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/General/Tags.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/General/StyleScrollViewer.xaml"/>                
                <ResourceDictionary Source="pack://application:,,,/Themes/DarkBlue.xaml"/>                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
