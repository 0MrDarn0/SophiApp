<Window x:Class="SophiAppCE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModel="clr-namespace:SophiAppCE.ViewModels"
        xmlns:controls="clr-namespace:SophiAppCE.Controls"
        xmlns:local="clr-namespace:SophiAppCE"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Name="AppWindow"
        Style="{DynamicResource SophiAppCE.MainWindow}">    
    <Window.DataContext>
        <viewModel:MainVM/>
    </Window.DataContext>
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="0" ResizeBorderThickness="3"/>
    </WindowChrome.WindowChrome>
    <Grid>
        
    </Grid>
</Window>
